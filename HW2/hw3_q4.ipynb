   "source": [
    "import numpy as np\n",
    "\n",
    "x = np.array([0, 1, 2, 3])\n",
    "y = np.array([1, 0, 2, -2])\n",
    "\n",
    "phi = np.array([\n",
    "    [0, 1],\n",
    "    [1, 1],\n",
    "    [2, 1],\n",
    "    [3, 1]\n",
    "])\n",
    "\n",
    "w = np.matmul(np.matmul(np.linalg.inv(np.matmul(phi.T, phi)), phi.T), y)\n",
    "\n",
    "print(f\"Closed form solution: {w}\")"
   ]
  }
